{
  "name": "meteor-emoji",
  "version": "0.1.0",
  "description": "A pure JS emoji picker because I couldn't find one that worked with meteor",
  "main": "dist/meteor-emoji.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-debug": "browserify src/meteorEmoji.js -s MeteorEmoji -t [babelify] -o dist/meteorEmoji.js",
    "build-min": "uglifyjs dist/meteorEmoji.js -c -o dist/meteorEmoji.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CreateMarketing/Meteor-Emoji-Picker.git"
  },
  "keywords": [
    "meteor-emoji-picker",
    "js-emoji",
    "emoji"
  ],
  "author": "George McCann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CreateMarketing/Meteor-Emoji-Picker/issues"
  },
  "homepage": "https://github.com/CreateMarketing",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "uglify-js": "^2.8.22"
  }
}
